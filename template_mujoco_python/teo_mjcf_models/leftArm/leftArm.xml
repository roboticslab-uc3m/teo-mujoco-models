                <inertial pos="-0.002272 0.056186 0.008165" mass="2" diaginertia="0.004226791 0.00212734 0.00409322"/>

                <!-- Visualización del cuerpo -->
                <geom name="FrontalLeftShoulder_visual_geom" type="mesh" mesh="FrontalLeftShoulder_visual" rgba="0.792157 0.819608 0.933333 1"/>

                <!-- Colisión del cuerpo -->
                <geom name="FrontalLeftShoulder_collision_geom" type="mesh" mesh="FrontalLeftShoulder_collision" conaffinity="1" condim="3" friction="0.7"/>

                <!-- Define la articulación -->
                <joint name="FrontalLeftShoulder" type="hinge" pos="0 0 0" axis="0 1 0" limited="true" range="-1.69 1.98" damping="0.001"/>

            </body>

            <body name="SagittalLeftShoulder_link" pos="0 0.084 0">

                <!-- Define las propiedades del cuerpo -->
                <inertial pos="-0.008047 0.004937 -0.027619" mass="1" diaginertia="0.001610921 0.002187768 0.001173531"/>

                <!-- Visualización del cuerpo -->
                <geom name="SagittalLeftShoulder_visual_geom" type="mesh" mesh="SagittalLeftShoulder_visual" rgba="0.792157 0.819608 0.933333 1"/>

                <!-- Colisión del cuerpo -->
                <geom name="SagittalLeftShoulder_collision_geom" type="mesh" mesh="SagittalLeftShoulder_collision" conaffinity="1" condim="3" friction="0.7"/>

                <!-- Define la articulación -->
                <joint name="SagittalLeftShoulder" type="hinge" pos="0 0 0" axis="1 0 0" limited="true" range="-0.42 1.34" damping="0.001"/>

            </body>

            <body name="AxialLeftShoulder_link" pos="0 0.084 -0.329">

