                <!-- Visualización del cuerpo -->
                <geom name="AxialLeftWrist_visual_geom" type="mesh" mesh="SagittalLeftShoulder_visual" rgba="0.792157 0.819608 0.933333 1"/>

                <!-- Colisión del cuerpo -->
                <geom name="AxialLeftWrist_collision_geom" type="mesh" mesh="SagittalLeftShoulder_collision" conaffinity="1" condim="3" friction="0.7"/>

                <!-- Define la articulación -->
