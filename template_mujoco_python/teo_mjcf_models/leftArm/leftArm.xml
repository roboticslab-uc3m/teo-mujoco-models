            </body>

        </body>
    </worldbody>

        <!-- Define los actuadores -->
        <actuator>
            <motor joint="FrontalLeftShoulder"/>
        </actuator>

        <actuator>
            <motor joint="SagittalLeftShoulder"/>
        </actuator>

        <actuator>
            <motor joint="AxialLeftShoulder"/>
        </actuator>

        <actuator>
            <motor joint="FrontalLeftElbow"/>
        </actuator>

        <actuator>
            <motor joint="AxialLeftWrist"/>
        </actuator>

        <actuator>
