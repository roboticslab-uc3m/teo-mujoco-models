    <link name='FrontalNeck_link'>
      <pose>0 0 0.0325 0 -0 0</pose>
      <inertial>
        <pose>-0.002729 -0.000086 0.067027 0 -0 0</pose>
        <mass>2</mass>
        <inertia>
          <ixx>0.007599322</ixx>
          <ixy>5.83e-09</ixy>
          <ixz>0.0000658</ixz>
          <iyy>0.008325273</iyy>
          <iyz>-7.43e-09</iyz>
          <izz>0.011403464</izz>
