<?xml version='1.0'?>
<sdf version='1.6'>
  <model name='TEO_head'>

    <link name='AxialNeck_link'>
      <pose>0 0 0 0 -0 0</pose>
      <inertial>
        <pose>0.000302 0.000193 -0.0452 0 -0 0</pose>
        <mass>1</mass>
        <inertia>
          <ixx>0.000939063</ixx>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyy>0.000939063</iyy>
          <iyz>0</iyz>
          <izz>0.000528125</izz>
        </inertia>
